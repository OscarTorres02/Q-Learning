      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import time\n",
        "\n",
        "# 1. Configuración del laberinto\n",
        "def generate_maze(size=100):\n",
        "    maze = np.zeros((size, size), dtype=int)\n",
        "\n",
        "    # Agregar obstáculos (1)\n",
        "    np.random.seed(42)\n",
        "    num_obstacles = int(size * size * 0.3)  # 30% del laberinto son obstáculos\n",
        "    for _ in range(num_obstacles):\n",
        "        x, y = np.random.randint(0, size, size=2)\n",
        "        maze[x, y] = 1\n",
        "\n",
        "    # Configurar objetivo (2) en una posición alcanzable\n",
        "    goal_x, goal_y = np.random.randint(0, size, size=2)\n",
        "    while maze[goal_x, goal_y] == 1:\n",
        "        goal_x, goal_y = np.random.randint(0, size, size=2)\n",
        "    maze[goal_x, goal_y] = 2\n",
        "\n",
        "    # Garantizar un camino despejado desde el inicio al objetivo\n",
        "    x, y = 0, 0\n",
        "    while (x, y) != (goal_x, goal_y):\n",
        "        maze[x, y] = 0  # Asegurar que el camino esté despejado\n",
        "        if x < goal_x:\n",
        "            x += 1\n",
        "        elif x > goal_x:\n",
        "            x -= 1\n",
        "        elif y < goal_y:\n",
        "            y += 1\n",
        "        elif y > goal_y:\n",
        "            y -= 1\n",
        "\n",
        "    return maze\n",
        "\n",
        "#2 Oscar\n",
        "maze = generate_maze(size=100)\n",
        "\n",
        "# Visualización inicial del laberinto\n",
        "plt.figure(figsize=(10, 10))\n",
        "plt.imshow(maze, cmap='hot', interpolation='nearest')\n",
        "plt.title(\"Laberinto 100x100\")\n",
        "plt.show()\n",
        "\n",
        "# 2. Definición de movimientos\n",
        "ACTIONS = {\n",
        "    0: (-1, 0),  # Arriba\n",
        "    1: (1, 0),   # Abajo\n",
        "    2: (0, -1),  # Izquierda\n",
        "    3: (0, 1)    # Derecha\n",
        "}\n",
        "\n",
        "def is_valid_move(maze, x, y):\n",
        "    if x < 0 or y < 0 or x >= maze.shape[0] or y >= maze.shape[1]:\n",
        "        return False\n",
        "    return maze[x, y] != 1  # No debe ser un obstáculo\n",
        "\n",
        "def move(maze, position, action):\n",
        "    dx, dy = ACTIONS[action]\n",
        "    new_x, new_y = position[0] + dx, position[1] + dy\n",
        "    if is_valid_move(maze, new_x, new_y):\n",
        "        return new_x, new_y\n",
        "    return position  # Si no es válido, se queda en la misma posición\n",
        "\n",
        "# 3. Implementación del algoritmo Q-Learning\n",
        "def q_learning(maze, start, goal, episodes=5000, alpha=0.1, gamma=0.95, epsilon=0.2):\n",
        "    q_table = np.zeros((*maze.shape, len(ACTIONS)))  # Q-Table\n",
        "\n",
        "    for episode in range(episodes):\n",
        "        position = start\n",
        "        while True:\n",
        "            # Elegir acción (Exploración vs Explotación)\n",
        "            if np.random.random() < epsilon:\n",
        "                action = np.random.choice(list(ACTIONS.keys()))  # Explorar\n",
        "            else:\n",
        "                action = np.argmax(q_table[position])  # Explotar\n",
        "\n",
        "            new_position = move(maze, position, action)\n",
        "            reward = 1 if maze[new_position] == 2 else -0.01  # Recompensa positiva para el objetivo\n",
        "\n",
        "            # Actualizar Q-Table\n",
        "            q_table[position][action] = (1 - alpha) * q_table[position][action] + \\\n",
        "                                         alpha * (reward + gamma * np.max(q_table[new_position]))\n",
        "\n",
        "            position = new_position\n",
        "\n",
        "            # Terminar si se alcanza la meta\n",
        "            if maze[position] == 2:\n",
        "                break\n",
        "\n",
        "        # Disminuir epsilon gradualmente\n",
        "        epsilon = max(0.1, epsilon * 0.99)\n",
        "\n",
        "    return q_table\n",
        "\n",
        "#Kevin\n",
        "# 4. Entrenamiento\n",
        "start_time = time.time()\n",
        "\n",
        "start = (0, 0)  # Posición inicial\n",
        "goal = tuple(map(tuple, np.argwhere(maze == 2)))[0]  # Identificamos la meta\n",
        "\n",
        "q_table = q_learning(maze, start, goal, episodes=5000)\n",
        "\n",
        "end_time = time.time()\n",
        "print(f\"Entrenamiento completado en {end_time - start_time:.2f} segundos\")\n",
        "\n",
        "# 5. Evaluación del agente\n",
        "def evaluate(maze, q_table, start, goal):\n",
        "    position = start\n",
        "    path = [position]\n",
        "\n",
        "    while position != goal:\n",
        "        action = np.argmax(q_table[position])\n",
        "        position = move(maze, position, action)\n",
        "        path.append(position)\n",
        "\n",
        "    return path\n",
        "\n",
        "path = evaluate(maze, q_table, start, goal)\n",
        "print(f\"Recorrido encontrado: {len(path)} pasos desde {start} hasta {goal}\")\n",
        "\n",
        "# 6. Visualización del recorrido\n",
        "def visualize_path(maze, path):\n",
        "    maze_copy = maze.copy()\n",
        "    for x, y in path:\n",
        "        if maze_copy[x, y] == 0:\n",
        "            maze_copy[x, y] = 9  # Marca el camino con 9\n",
        "\n",
        "    plt.figure(figsize=(10, 10))\n",
        "    plt.imshow(maze_copy, cmap='hot', interpolation='nearest')\n",
        "    plt.title(\"Recorrido del Agente en Laberinto 100x100\")\n",
        "    plt.show()\n",
        "\n",
        "visualize_path(maze, path)"
      ]
    }
  ]
}